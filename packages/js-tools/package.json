{
  "name": "@superdispatch/js-tools",
  "version": "0.1.47",
  "description": "CLI tools for JavaScript",
  "keywords": [
    "eslint",
    "prettier"
  ],
  "repository": "https://github.com/superdispatch/js-tools.git",
  "license": "MIT",
  "files": [
    "src/bin.ts",
    "src/index.ts",
    "src/commands/**"
  ],
  "main": "src/index.ts",
  "bin": {
    "js-tools": "./pkg/dist-node/index.bin.js"
  },
  "scripts": {
    "build": "pika build"
  },
  "dependencies": {
    "execa": "^4.0.0",
    "prettier-plugin-packagejson": "^2.0.10",
    "yargs": "^15.1.0",
    "yarn-deduplicate": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "^6.0.0",
    "prettier": "^1.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "publishConfig": "pkg"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "js-tools"
        }
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
