{
  "name": "@superdispatch/js-tools",
  "version": "0.1.50",
  "description": "CLI tools for JavaScript",
  "keywords": [
    "eslint",
    "prettier"
  ],
  "repository": "https://github.com/superdispatch/js-tools.git",
