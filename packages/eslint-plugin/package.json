{
  "name": "@superdispatch/eslint-plugin",
  "version": "0.1.50",
  "description": "Shared ESLint plugin",
  "keywords": [
    "eslint"
  ],
  "repository": "https://github.com/superdispatch/js-tools.git",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src/index.js",
    "src/rules/**",
    "src/configs/**",
    "!**/__tests__/**",
    "!**/__testutils__/**"
  ],
  "scripts": {
    "typecheck": "yarn tsc"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "confusing-browser-globals": "1.x",
    "eslint-config-prettier": "6.x",
    "eslint-plugin-array-func": "3.x",
    "eslint-plugin-eslint-comments": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "23.x",
    "eslint-plugin-node": "11.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "eslint-plugin-simple-import-sort": "5.x",
    "lodash": "4.x"
  },
  "peerDependencies": {
    "eslint": "^6.7.0",
    "prettier": "^1.19.0",
    "typescript": "^3.7.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
