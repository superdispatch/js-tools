{
  "name": "@superdispatch/gh-scripts",
  "version": "0.2.3",
  "description": "Node scripts for GitHub actions",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/superdispatch/js-tools",
  "bugs": "https://github.com/superdispatch/js-tools/issues",
  "repository": "superdispatch/js-tools",
  "license": "MIT",
  "author": "doniyor2109 @doniyor2109",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "gh-scripts": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "netlify-cli": "^2.64.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gh-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
